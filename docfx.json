{
  "$schema": "https://dotnet.github.io/docfx/schemas/v1.0/schema.json#",
  "metadata": [
    {
      "src": [
        {
          "src": "src",
          "files": [
            "**/*.csproj"
          ]
        }
      ],
      "dest": "api"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "**/*.{md,yml}"
        ],
        "exclude": [
          "_site/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "output": "_site",
    "templates": [
      "default",
      "modern"
    ],
    "globalMetadata": {
      "_appName": "net-questdb-client",
      "_appTitle": "net-questdb-client",
      "_enableSearch": true,
      "pdf": false
    }
  }
}
